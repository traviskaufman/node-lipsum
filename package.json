{
  "author": "Travis Kaufman <travis.kaufman@gmail.com>",
  "name": "node-lipsum",
  "type": "module",
  "version": "1.0.1",
  "description": "Web service for lipsum.com",
  "keywords": [
    "Dummy Text",
    "Lorem Ipsum",
    "Lipsum",
    "Mock",
    "Fixtures"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/traviskaufman/node-lipsum"
  },
  "bin": {
    "node-lipsum": "./dist/cli.js"
  },
  "preferGlobal": true,
  "main": "./lib/lipsum",
  "dependencies": {
    "argparse": "0.1.x",
    "coffee-script": "1.x.x",
    "nock": "^14.0.10",
    "xml2js": "0.1.x"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/xml2js": "^0.4.14",
    "coffeedoc": "0.2.x",
    "coffeelint": "0.4.x",
    "husky": "^9.1.7",
    "jasmine-node": "1.0.x",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "shelljs": "0.0.x",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "pretest": "npm run clean && tsc --sourceMap false --inlineSourceMap",
    "test": "vitest run",
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown"
  }
